{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "PAV",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/main.bin",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Habilitar pretty-printing para GDB",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "compilar",
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
